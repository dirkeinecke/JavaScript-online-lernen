<?xml version='1.0' encoding='UTF-8'?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">

  {% for html_file in site.html_files %}
    {% if html_file.sitemap != null and html_file.sitemap != empty %}
      <url>
        <loc>{{ site.url }}{{ html_file.url }}</loc>
        <lastmod>{{ html_file.sitemap.lastmod | date_to_xmlschema }}</lastmod>
        <changefreq>{{ html_file.sitemap.changefreq }}</changefreq>
        <priority>{{ html_file.sitemap.priority }}</priority>
       </url>
    {% endif %}
  {% endfor %}

